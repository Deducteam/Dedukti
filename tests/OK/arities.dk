(; OK ;)
A : Type.

a : A.
b : A.
c : A.
d : A.

g : A -> A.
h : A -> A -> A -> A.
i : A -> A -> A -> A -> A.

def f : A -> A -> A -> A -> A.
[] f a a a --> g.
[] f b --> h.
[] f --> i.


(;# assert  f a a a b == g b #;)
(;# assert  f c a b == i c a b #;)
(;# assert  f b a b == h a b #;)
(;# assert  f b a == h a #;)
(;# assert  f a a == i a a #;)
(;# assert  f a == i a #;)
(;# assert  f == i #;)
