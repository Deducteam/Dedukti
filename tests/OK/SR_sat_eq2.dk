(; OK --type-lhs ;)

A : Type.
a : A.
c : A -> A -> A.

T : A -> Type.
t : x : A -> T x.

def f : A -> A.


def   test1 : x : A -> T (c (f x) a)   -> T (f (f x)).
[X,Y] test1   X       (t (c Y     a)) --> t (f Y    ).

def   test2 : x : A -> T (c (f x) a)   -> T (f (f x)).
[X,Y] test2   X       (t (c (f Y) a)) --> t (f (f Y)).

def   test3 : x : A -> T (f (c (f x) (f (f x))))  -> T (f (c (f x) (f (f x)))).
[X,Y] test3   X       (t (f Y)                 ) --> t (f Y).

def   test4 : x : A -> T (c a (f (c (f x) (f (f x)))))  -> T (f (c (f x) (f (f x)))).
[X,Y] test4   X       (t (c a (f Y)                 )) --> t (f Y).
